{
    "manifest_version": 2,
    "name": "GooseGooseGo",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://duckduckgo.com/*", "http://duckduckgo.com/*"
          ],
          "js": ["./lib/content.js"],
          "css": ["./lib/styles.css"]
        }
      ],
    "browser_action": {
        "default_icon": "./img/icon.png"
    }
}   