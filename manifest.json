{
    "manifest_version": 2,
    "name": "GooseGooseGo",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://duckduckgo.com/*", "http://duckduckgo.com/*"
          ],
          "js": ["./lib/js/content.js", "./lib/html/sticker.js", "./lib/html/sticker.min.js"],
          "css": ["./lib/styles/styles.css", "./lib/html/popup_styles.css"]
        }
      ],
    "browser_action": {
        "default_icon": "./img/icon.png",
        "default_popup": "./lib/html/popup.html"
    }
    
}   