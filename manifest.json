{
    "manifest_version": 2,
    "name": "GooseGooseGo",
    "version": "0.1",
    "content_scripts": [
        {
          "matches": [
            "https://duckduckgo.com/*", "http://duckduckgo.com/*"
          ],
          "js": ["content.js"]
        }
      ],
    "browser_action": {
        "default_icon": "./images/icon.png"
    }
}   